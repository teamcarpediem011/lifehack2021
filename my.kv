<LandingPage>
    name: "landing"

    Image:
        source:"logo.png"
        size_hint: 0.3, 0.5
        pos_hint: {"x":0.35, "y":0.39}

    FloatLayout:
        cols: 1
        Label:
            text: "What's your name?"
            font_size: 20
            pos_hint: {"x":0, "y":-0.12}

        TextInput:
            multiline: False
            id: username
            size_hint: 0.3, 0.05
            pos_hint: {"x":0.36, "y":0.3}

    Button:
        text:"Find a\nbuddy!"
        size_hint: 0.2, 0.1
        pos_hint: {"x":0.42, "y":0.15}
        font_size: 20
        on_release:
            root.manager.transition.direction = "left"
            root.login()
